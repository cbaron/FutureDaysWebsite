{
  "description": "Site for Future Days",
  "engines": {
    "node": "6.2.0"
  },
  "dependencies": {
    "backbone": "^1.3.1",
    "jquery": "^2.2.4",
    "jws": "^3.1.3",
    "moment": "^2.12.0",
    "node-env-file": "^0.1.8",
    "pg": "4.5.5",
    "pg-native": "^1.10.0",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babelify": "7.3.0",
    "browserify": "13.0.1",
    "parallelshell": "2.0.0",
    "stylus": "0.54.0",
    "watch": "0.18.0"
  },
  "license": "BSD-3-Clause",
  "name": "FutureDays",
  "repository": {
    "type": "git",
    "url": "https://github.com/cbaron/futuredays.git"
  },
  "scripts": {
    "build:css": "stylus -w stylus/main.styl -o static/css/main.css",
    "build:js": "browserify client/js/main.js -o static/js/debug.js -d -t [ babelify --presets [ es2015 ] ]",
    "build:watch": "parallelshell 'npm run watch:css' 'npm run watch:js'",
    "livereload": "live-reload --port 9091 static/",
    "watch:css": "watch 'npm run build:css' stylus --ignoreDotFiles",
    "watch:js": "watch 'npm run build:js' client/js --ignoreDotFiles"
  },
  "version": "0.0.1"
}
